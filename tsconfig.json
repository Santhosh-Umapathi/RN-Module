{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "jsx": "react-native",
    "types": ["react-native"],
    "outDir": "dist",
    "declaration": true,
    "esModuleInterop": true,
    "baseUrl": "."
  },
  "include": ["lib/**/*"],
  "exclude": ["node_modules"]
}
